{
  "name": "portfolio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "parcel src/index.html --open",
    "build": "parcel build src/index.html --dist-dir docs --public-url ./",
    "clean": "rimraf .parcel-cache docs dist",
    "deploy:main": "npm run build && echo.> docs\\.nojekyll && copy /Y docs\\index.html docs\\404.html"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.16.1",
    "parcel": "^2.12.0",
    "rimraf": "^5.0.5",
    "sass": "^1.79.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "scrollreveal": "^4.0.9"
  }
}
